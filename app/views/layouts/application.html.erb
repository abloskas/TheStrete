<!DOCTYPE html>
<html>
<head>
  <title>TheStrete</title>
  
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag "style.css" %>
  <%= stylesheet_link_tag "login_style.css" %>
  <%= stylesheet_link_tag "https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js'%>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="errors">
<% if flash[:errors] %>
  <% flash[:errors].each do |x| %>
    <%= x %>
  <% end %>
<% end %>
</div>



<%= yield %>


</body>
</html>
